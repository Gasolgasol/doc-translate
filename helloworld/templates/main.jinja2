                        <table class="table">
  <thead class="thead-light">
    <tr>
      <th scope="col">#</th>
      <th scope="col">File name</th>
      <th scope="col">Size</th>
      <th scope="col">Original file</th>
      <th scope="col">Translation</th>
    </tr>
  </thead>
  <tbody>

    {% for item in list %} 
    <tr>
      <th scope="row"><div class="toalign">{{ item.id }}</div></th>
      <td><div class="toalign">{{ item.name }}</div></td>
      <td><div class="toalign">{{ item.size }}</div></td>
      <td><form method="get" action="/download/{{ item.server_name }}/{{item.name}}/first"><button type="submit" class="btn btn-light btn-sm">download</button></form></td>
      <td><form method="get" action="/download/{{ item.server_name }}/{{item.name}}/second"><button type="submit" class="btn btn-light btn-sm">download</button></form></td>
    </tr>
    {% endfor %}
    </tr>
  </tbody>
</table>
                    </div>
                </div>
            </div>
        </section>
